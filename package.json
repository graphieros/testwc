{
  "name": "wc-svg-annotator",
  "version": "0.2.5",
  "description": "A web component you can use to select and annotate any web content",
  "author": {
    "name": "Alec Lloyd Probert"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphieros/testwc"
  },
  "license": "MIT",
  "keywords": [
    "web component",
    "annotator",
    "svg annotator",
    "svg",
    "annotation",
    "data annotation"
  ],
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/svg-annotator.js",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "svg-annotator": "^0.4.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
